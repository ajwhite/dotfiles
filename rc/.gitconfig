[user]
  name = Atticus White
  email = contact@atticuswhite.com
[github]
  user = ajwhite
[alias]
  ci = commit
  ch = checkout
  st = status
  s = status -s
  br = branch
  df = diff
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
  revision = rev-parse HEAD
  diffpager = !git diff --color=always | less -R
  dfp = !git diffpager
  diffp = !git diffpager
  fall = fetch --all
[push]
  default = matching
[core]
  excludesfile = ~/.gitignore_global
  pager = less -FRSX
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
